package com.revature.services;

import java.util.List;

import com.revature.daos.SurveyRequestDAO;
import com.revature.daos.methods.UserRequestDAOMethods;
import com.revature.model.UserRequest;

public class SurveyRequestService {
	
	SurveyRequestDAO srdao = new UserRequestDAOMethods();
	
	public List<UserRequest> getAllRequests() {
		return srdao.getSurveyRequests();
		
	}
	
	public int createUserRequest(UserRequest sr) {
		return srdao.createSurveyRequest(sr);
	}
	

}
